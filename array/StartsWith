public class startswith {
    public static boolean find(String s , String a , int idx){
        if(idx==a.length()){
            return true;
        }
        if(s.charAt(idx)!=a.charAt(idx)){
            return false;
        }else{
            return find(s, a, idx+1);
        }
    }
    public static void main (String[] args){
        String s ="abcde";
        String a ="ad";
        int idx=0;
        System.out.println(find(s, a, idx));
    }
}
