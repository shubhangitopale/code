package Date10062024;
  public class _11 {
  	public static void main(String[] args) {
  		int[] arr = {1,5,45,78,96,33,21,6};
    		for (int i = 0 ; i < arr.length-1 ; i++) {
    			int temp = arr[i];
    			arr[i]=arr[i+1];
    			arr[i+1]=temp;
    		}
    		for (int i = 0; i < arr.length; i++) {
    			System.out.print(arr[i]+" ");
    		} 
 	  }
}
