package Date10062024;
  public class Avg_inner_elemets {
  	public static void main(String[] args) {
    		int[][] arr = {
    				{1,2,3},
    				{4,5,6},
    				{7,8,9}
    		};
    		int sum =0;
    		int rows = arr.length;
    		int columns = arr[0].length;
    		int count=0;
      		for (int i = 1; i < rows-1; i++) {
      			for (int j = 1; j < columns-1 ; j++) {
      				sum += arr[i][j];
      				count++;
      			}
      		}
    		System.out.println("AVG:"+sum/count);
  	}
}
