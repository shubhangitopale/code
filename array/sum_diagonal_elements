package Date10062024;
  public class sum_diagonal_elements {
  	public static void main(String[] args) {
  		int[][] arr = {
  				{2,5,6},
  				{4,5,6},
  				{2,5,10}
  		};
  		int n = arr.length;
  		int m = arr[0].length;
  		int sum=0;
    		for (int i = 0; i < n; i++) {
    			for (int j = 0; j < m; j++) {
    				if(i==j) {
    					sum+=arr[i][j];
    				}
    			}
    		}
    		System.out.println(sum);
  	}
}
