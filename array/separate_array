package Date10062024;
  public class separate_array {
  	public static void main(String[] args) {
  		int[] arr = {1,-5,-44,-6,4,9,-3};
  		int n = arr.length;
    		for (int i = 0; i < n; i++) {
    			if(arr[i]<0) {
      			for (int j = i; j < n; j++) {
      				if(arr[j]>0) {
      					int temp = arr[i];
      					arr[i]=arr[j];
      					arr[j]=temp;  
      				  }
      			 }
    		   }
    		}
  		for(int i=0 ; i<n ;i++) {
  			System.out.print(arr[i]+" ");
  		}
  }
}
