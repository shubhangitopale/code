package Date10062024;
  public class maxsum {
  	public static void main(String[] args) {
  		int[] arr = {5,1,3,4,7};
  		int n = arr.length;
  		int count=0;
  		
  		for (int i = 0; i < n; i++) {
  			for (int j = i+1; j < n; j++) {
  				for (int j2 = j+1; j2 < n; j2++) {
  					if(arr[i]+arr[j]+arr[j2]<12) {
  						count++;
  					}
  				}
  			}
  		}
  		System.out.println(count);
  	}
}
