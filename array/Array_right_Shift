package Date10062024;
  public class right_shift {
  	public static void main(String[] args) {
    		for (int i = arr.length-1 ; i > 0  ; i--) {
    			int temp = arr[i];
    			arr[i]=arr[i-1];
    			arr[i-1]=temp;
    		}
    		for (int i = 0; i < arr.length; i++) {
    			System.out.print(arr[i]+" ");
    		}
  	}
}
