public class charArray {
    public static void array(String s, int idx) {
        if (idx == s.length()) {
            return;
        }
        System.out.print(s.charAt(idx));
        System.out.print(" ");
        array(s, idx + 1);
    }

    public static void main(String[] args) {
        String s = "shubhangi";
        int idx = 0;
        System.err.print("[ ");
        array(s, idx);
        System.err.print("]");
    }
}
