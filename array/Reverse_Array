package Date10062024;
  public class reverse {
    	public static void main(String[] args) {
      		int[] arr1 = {1,5,4,44,55};
      		int n =arr1.length;
      		int temp = 0;
        		for (int j = 0 ; j < n/2 ; j++) {
        			temp = arr1[j];
        			arr1[j]=arr1[n-j-1];
        			arr1[n-j-1] = temp;
        			
        		}
          		for(int i=0 ; i<arr1.length ; i++) {
          			System.out.print(arr1[i]+" ");
          		}		
    	}
  }
